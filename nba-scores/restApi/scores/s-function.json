{
  "name": "scores",
  "runtime": "nodejs",
  "description": "Serverless Lambda function for project: nba-scores",
  "customName": false,
  "customRole": false,
  "handler": "scores/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "envVars": [],
    "optimize": true,
    "cors": {
      "allowOrigin": "*"
    }
  },
  "endpoints": [
    {
      "path": "scores",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {},
          "application/json": ""
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "BEARER_TOKEN": "${bearer_token}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
